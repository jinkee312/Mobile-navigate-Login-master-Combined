<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.navigation">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".LoginMain" />
        <activity android:name=".StaffLogin" />
        <activity android:name=".StudentLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NavActivity"/>
        <activity android:name=".StudentRegister" />
        <activity android:name=".StaffRegister" />
        <activity android:name=".NewMessage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NewMessage" />
        </activity>
        <activity android:name=".chat">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NewMessage" />
        </activity>

    </application>

</manifest>